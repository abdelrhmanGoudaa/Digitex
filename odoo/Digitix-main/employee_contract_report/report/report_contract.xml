<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
<template id="report_contract_document">
     <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <h2>عقد عمل <span t-field="o.contract_serial_number"/></h2>
                            <table class="table table-condensed">
                                <thead>
                                    <tr>
                                        <th>الموظف</th>
                                        <th>تاريخ بداية العقد</th>
                                        <th>تاريخ نهاية العقد</th>
                                         <th>القسم</th>
                                          <th>الوظيفة</th>

                                        <!-- Add more fields as needed -->
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><t t-esc="o.employee_id.name"/></td>
                                        <td><t t-esc="o.date_start"/></td>
                                        <td><t t-esc="o.date_end"/></td>
                                         <td><t t-esc="o.department_id.name"/></td>
                                            <td><t t-esc="o.job_id.name"/></td>
                                       
                                        <!-- Add more field values -->
                                    </tr>
                                </tbody>
                            </table>
                            <p><t t-esc="o.company_id.x_studio_contract"/></p>
                        </div>
                    </t>
                </t>
            </t>
</template>


<template id="report_contract_documenttt">
    <t t-call="report.html_container">
    <t t-foreach="docs" t-as="doc">
            <t t-call="report_contract_document" t-lang="doc.employee_id.lang"/>
        </t>
    </t>
</template>
</data>
</odoo>
