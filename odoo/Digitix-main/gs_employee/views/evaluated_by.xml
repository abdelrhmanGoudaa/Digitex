<odoo>

    <record id="gs_evaluated_by_view_tree" model="ir.ui.view">
        <field name="name">gs.evaluated.by.view.tree</field>
        <field name="model">gs.evaluated.by</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="gs_evaluated_by_view_form" model="ir.ui.view">
        <field name="name">gs.evaluated.by.view.form</field>
        <field name="model">gs.evaluated.by</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="gs_evaluated_by_action" model="ir.actions.act_window">
        <field name="name">Evaluated By</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">gs.evaluated.by</field>
        <field name="view_mode">tree,form</field>
    </record>

