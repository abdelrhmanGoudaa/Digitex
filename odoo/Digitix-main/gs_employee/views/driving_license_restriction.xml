<odoo>

    <record id="gs_driving_license_restriction_view_tree" model="ir.ui.view">
        <field name="name">gs.driving.license.restriction.view.tree</field>
        <field name="model">gs.driving.license.restriction</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="gs_driving_license_restriction_view_form" model="ir.ui.view">
        <field name="name">gs.driving.license.restriction.view.form</field>
        <field name="model">gs.driving.license.restriction</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="gs_driving_license_restriction_action" model="ir.actions.act_window">
        <field name="name">Driving License Restriction</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">gs.driving.license.restriction</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>